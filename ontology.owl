<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/def/astrea#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:domain :Component ;
          rdfs:range :OWLConstruct ;
          rdfs:label "Relation between the component and the set of constructs that includes" ,
                     "contains" .


###  https://w3id.org/def/astrea#hasMapping
:hasMapping rdf:type owl:ObjectProperty ;
            owl:inverseOf :isMappedFrom ;
            rdfs:comment "Relation between a component and its mapping" ;
            rdfs:label "has mapping" .


###  https://w3id.org/def/astrea#isExtractedBy
:isExtractedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :MappingRelation ;
               rdfs:label "is extracted by" ,
                          "relation between the mapping and the SPARQL query needed to extract the information" .


###  https://w3id.org/def/astrea#isMappedFrom
:isMappedFrom rdf:type owl:ObjectProperty ;
              rdfs:domain :MappingRelation ;
              rdfs:comment "Relation between the mapping and the source component" ;
              rdfs:label "is mapped from" .


###  https://w3id.org/def/astrea#isMappedTo
:isMappedTo rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :MappingRelation ;
            rdfs:comment "Relation between the mapping and the result component" ;
            rdfs:label "is mapped to" .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/def/astrea#body
:body rdf:type owl:DatatypeProperty ;
      rdfs:domain :SPARQLQuery ;
      rdfs:range xsd:string ;
      rdfs:comment "body of the SPARQL query" ;
      rdfs:label "body" .


###  https://w3id.org/def/astrea#category
:category rdf:type owl:DatatypeProperty ;
          rdfs:domain :Construct ;
          rdfs:range xsd:string ;
          rdfs:comment "category of a construct" ;
          rdfs:label "category" .


###  https://w3id.org/def/astrea#order
:order rdf:type owl:DatatypeProperty ;
       rdfs:domain :SPARQLQuery ;
       rdfs:range xsd:integer ;
       rdfs:comment "Order of the query" ;
       rdfs:label "order" .


###  https://w3id.org/def/astrea#syntax
:syntax rdf:type owl:DatatypeProperty ;
        rdfs:domain :Construct ;
        rdfs:range xsd:string ;
        rdfs:comment "syntax of the construct" ;
        rdfs:label "syntax" .


###  https://w3id.org/def/astrea#topic
:topic rdf:type owl:DatatypeProperty ;
       rdfs:domain :Component ;
       rdfs:range xsd:string ;
       rdfs:comment "topic of a component" ;
       rdfs:label "topic" .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/def/astrea#Component
:Component rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasMapping ;
                             owl:someValuesFrom :MappingRelation
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :contains ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Construct
                           ] ;
           rdfs:comment "Set of constructs of a language related to a particular topic" ;
           rdfs:label "Component" .


###  https://w3id.org/def/astrea#Construct
:Construct rdf:type owl:Class ;
           rdfs:label "Construct" .


###  https://w3id.org/def/astrea#MappingRelation
:MappingRelation rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :isMappedFrom ;
                                   owl:someValuesFrom :Component
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :isMappedTo ;
                                   owl:someValuesFrom :Construct
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :isExtractedBy ;
                                   owl:allValuesFrom :SPARQLQuery
                                 ] ;
                 rdfs:label "Mapping Relation" .


###  https://w3id.org/def/astrea#OWLConstruct
:OWLConstruct rdf:type owl:Class ;
              rdfs:subClassOf :Construct ;
              rdfs:comment "Construct in OWL language" ;
              rdfs:label "OWL Construct" .


###  https://w3id.org/def/astrea#SHACLConstruct
:SHACLConstruct rdf:type owl:Class ;
                rdfs:subClassOf :Construct ;
                rdfs:comment "Construct in SHACL language" ;
                rdfs:label "SHACL Construct" .


###  https://w3id.org/def/astrea#SPARQLQuery
:SPARQLQuery rdf:type owl:Class ;
             rdfs:comment "Query in SPARQL that is needed to extract the information of the components to do the mapping" ;
             rdfs:label "SPARQL query" .


