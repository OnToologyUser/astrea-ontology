            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/astrea#contains"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/def/astrea#Construct"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Set of constructs of a language related to a particular topic</rdfs:comment>
        <rdfs:label>Component</rdfs:label>
    </owl:Class>
    


