    <!-- https://w3id.org/def/astrea#Component -->


    <owl:Class rdf:about="https://w3id.org/def/astrea#Component">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/astrea#hasMapping"/>
